{
  "headers": [
    {
      "source": "/serviceworker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/taxi", "destination": "/" },
    { "source": "/shop", "destination": "/" },
    { "source": "/(.*)", "destination": "/$1" }
  ]
}
