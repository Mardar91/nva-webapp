{
  "version": 2,
  "headers": [
    {
      "source": "/serviceworker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/taxi",
      "destination": "/"
    },
    {
      "source": "/shop",
      "destination": "/"
    },
    {
      "source": "/:path*/pwa=:value",
      "destination": "/:path*"
    },
    {
      "source": "/:path*\\?pwa=:value",
      "destination": "/:path*"
    },
    {
      "source": "/:path*\\?source=:value",
      "destination": "/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
