{
  "headers": [
    {
      "source": "/pwa-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/push/onesignal/OneSignalSDKWorker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/taxi", "destination": "/" },
    { "source": "/shop", "destination": "/" },
    { "source": "/api/schedule-notification", "destination": "/api/schedule-notification" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "functions": {
    "api/schedule-notification.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
